<Window x:Class="Lection0506.Views.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lection0506.Views"
        xmlns:vm="clr-namespace:Lection0506.ViewModels"
        mc:Ignorable="d"
        Title="GameWindow" Height="450" Width="800">
    <!--<Window.DataContext>
        <vm:GameViewModel/>
    </Window.DataContext>-->
    <StackPanel>
        <TextBox Text="{Binding SelectedGame.IdGame}"/>
        <TextBox Text="{Binding SelectedGame.Name}"/>
        <TextBox Text="{Binding SelectedGame.Price}"/>
    </StackPanel>
</Window>
