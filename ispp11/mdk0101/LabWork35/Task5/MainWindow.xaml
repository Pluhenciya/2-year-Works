<Window x:Class="Task5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Task5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <Button x:Name="PrintButton" Content="Печать" DockPanel.Dock="Bottom" HorizontalAlignment="Right" Margin="10"
                Click="PrintButton_Click"/>
        <Grid>
            <Canvas x:Name="ContentCanvas" Background="AliceBlue">
                <Label Content="Label" Canvas.Left="42" Canvas.Top="100" HorizontalAlignment="Center" VerticalAlignment="Top"/>
                <TextBlock Canvas.Left="26" TextWrapping="Wrap" Text="TextBlock" Canvas.Top="40" RenderTransformOrigin="0.434,2.359" HorizontalAlignment="Center" VerticalAlignment="Top"/>
                <TextBox Canvas.Left="662" TextWrapping="Wrap" Text="TextBox" Canvas.Top="17" Width="120"/>
                <CheckBox Content="CheckBox" Canvas.Left="565" Canvas.Top="19"/>
                <TextBox Canvas.Left="41" TextWrapping="Wrap" Text="TextBox" Canvas.Top="130" Width="701" Height="232"/>
                <Ellipse Height="100" Canvas.Left="5" Stroke="Black" Width="100"/>
            </Canvas>
        </Grid>
    </DockPanel>
</Window>
